generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Peserta {
  id         Int     @id @default(autoincrement())
  nik        String  @unique
  nama       String
  status     Boolean
  departemen String?
}

model Hadiah {
  id       Int     @id @default(autoincrement())
  nama     String
  no_urut  Int
  pemenang Int?
  img_url  String?
}
